{
  "name": "discord-activity-boilerplate",
  "version": "1.0.0",
  "description": "a simple boilerplate for discord activity",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start:server": "node start/server.js",
    "start:client": "node start/client.js",
    "start:all": "concurrently \"pnpm run start:server\" \"pnpm run start:client\"",
    "start": "node index.js",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": {
    "name": "Jonas Franke <@binary-blazer>",
    "email": "jonasfranke@sdevs.org"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "child_process": "^1.0.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "prettier": "3.2.5"
  }
}
